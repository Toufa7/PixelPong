    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.2.1",
    "@nestjs/platform-socket.io": "^10.2.7",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/websockets": "^10.2.7",
    "@prisma/client": "^5.1.1",
    "@types/node-fetch": "^2.6.6",
